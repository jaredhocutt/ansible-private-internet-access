[connection]
id=PIA - Netherlands
uuid=e960c347-d95b-44ae-8396-b29f7f1ad55e
type=vpn
autoconnect=false

[vpn]
service-type=org.freedesktop.NetworkManager.openvpn
username={{ pia_username }}
comp-lzo=yes
remote=nl.privateinternetaccess.com
cipher=AES-256-CBC
