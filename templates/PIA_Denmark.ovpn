[connection]
id=PIA - Denmark
uuid=ec30372b-de39-4e8f-bb42-ed5cf1d18374
type=vpn
autoconnect=false

[vpn]
service-type=org.freedesktop.NetworkManager.openvpn
username={{ pia_username }}
comp-lzo=yes
remote=denmark.privateinternetaccess.com
cipher=AES-256-CBC
auth=SHA256
connection-type=password
password-flags=1
port=1197
proto-tcp=no
ca=/etc/openvpn/pia-ca.rsa.4096.crt

[ipv4]
method=auto
